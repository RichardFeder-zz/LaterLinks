{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "LaterLinks",
  "description": "Avoid a cluttered browser window by saving links and sessions to view later with one time access!",
  "version": "0.1",
  "background": {"scripts": ["background.js", "jquery-1.11.3.min.js"]},
  "minimum_chrome_version": "16.0.884",
  "icons": {
    "16": "img/icon_16.png",
    "32": "img/icon_32.png",
    "64": "img/icon_64.png",
    "128":"img/icon_128.png"
  },
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'",
  "permissions": ["<all_urls>", "storage","http://*/*", "https://*/*", "tabs", "bookmarks", "contextMenus"],
  "browser_action": {"default_icon": "img/icon_32.png", "default_popup": "popup.html"},
  "manifest_version": 2
}